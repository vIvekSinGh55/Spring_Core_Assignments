<?xml version="1.0" encoding="UTF-8"?>

<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:context="http://www.springframework.org/schema/context"
       xsi:schemaLocation="http://www.springframework.org/schema/beans 
           http://www.springframework.org/schema/beans/spring-beans-3.0.xsd
           http://www.springframework.org/schema/context
           http://www.springframework.org/schema/context/spring-context-3.0.xsd">
               
     <context:annotation-config/>
     
    
    <bean id="myinfo" class="com.question5.Info">
        <property name="name" value="Dontknow" />
        <property name="location" value="India" />
    </bean>
 
     <bean id="mycustomer" class="com.question5.Customer">
          <property name="id" value="1111" /> 
        <property name="name" value="Unknown" />
        </bean> 
        
        <bean class="org.springframework.beans.factory.annotation.RequiredAnnotationBeanPostProcessor" />
  
</beans>